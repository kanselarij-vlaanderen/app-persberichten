{
  "version": "0.1",
  "prefixes": {
    "nco": "http://www.semanticdesktop.org/ontologies/2007/03/22/nco#",
    "vcard": "http://www.w3.org/2006/vcard/ns#"
  },
  "resources": {
    "contact-lists": {
      "new-resource-base": "http://themis.vlaanderen.be/id/contact-lijst/",
      "class": "nco:ContactList",
      "attributes": {
        "name": {
          "type": "string",
          "predicate": "vcard:fn"
        },
        "modified": {
          "type": "datetime",
          "predicate": "dct:modified"
        }
      },
      "relationships": {
        "contact-items": {
          "predicate": "nco:containsContact",
          "target": "contact-items",
          "cardinality": "many"
        },
        "publication-events": {
          "predicate": "ext:contactListHasChannelPublicationEvent",
          "target": "publication-events",
          "cardinality": "many"
        },
        "press-releases": {
          "predicate": "ext:contactLists",
          "target": "press-releases",
          "cardinality": "many",
          "inverse": true
        }
      },
      "features": [
        "include-uri"
      ]
    },
    "contact-items": {
      "new-resource-base": "http://themis.vlaanderen.be/id/contact-item/",
      "class": "nco:Contact",
      "attributes": {
        "full-name": {
          "type": "string",
          "predicate": "vcard:fn"
        },
        "given-name": {
          "type": "string",
          "predicate": "vcard:given-name"
        },
        "family-name": {
          "type": "string",
          "predicate": "vcard:family-name"
        },
        "organization-name": {
          "type": "string",
          "predicate": "vcard:organization-name"
        }
      },
      "relationships": {
        "contact-list": {
          "predicate": "nco:ContactList",
          "target": "contact-lists",
          "cardinality": "one",
          "inverse": true
        },
        "telephone": {
          "predicate": "vcard:hasTelephone",
          "target": "telephones",
          "cardinality": "one"
        },
        "mail-address": {
          "predicate": "vcard:hasEmail",
          "target": "mail-addresses",
          "cardinality": "one"
        },
        "publication-events": {
          "predicate": "ext:contactHasChannelPublicationEvent",
          "target": "publication-events",
          "cardinality": "many"
        },
        "press-releases": {
          "predicate": "ext:contacts",
          "target": "press-releases",
          "cardinality": "many",
          "inverse": true
        }
      },
      "features": [
        "include-uri"
      ]
    }
  }
}
