{
  "version": "0.1",
  "prefixes": {
    "dct": "http://purl.org/dc/terms/",
    "ext": "http://mu.semte.ch/vocabularies/ext/",
    "mu": "http://mu.semte.ch/vocabularies/core/",
    "foaf": "http://xmlns.com/foaf/0.1/"
  },
  "resources": {
    "users": {
      "new-resource-base": "http://themis.vlaanderen.be/id/gebruiker/",
      "class": "foaf:Person",
      "attributes": {
        "first-name": {
          "type": "string",
          "predicate": "foaf:firstName"
        },
        "family-name": {
          "type": "string",
          "predicate": "foaf:familyName"
        }
      },
      "features": [
        "include-uri"
      ],
      "relationships": {
        "account": {
          "predicate": "foaf:account",
          "target": "accounts",
          "cardinality": "one"
        },
        "group": {
          "predicate": "foaf:member",
          "target": "user-groups",
          "cardinality": "one",
          "inverse": true
        },
        "token-claims": {
          "predicate": "prov:wasAttributedTo",
          "target": "token-claims",
          "cardinality": "many",
          "inverse": true
        },
        "press-release-activities": {
          "predicate": "dct:creator",
          "target": "press-release-activities",
          "cardinality": "many",
          "inverse": true
        }
      }
    },
    "user-groups": {
      "new-resource-base": "http://themis.vlaanderen.be/id/gebruikersgroep/",
      "class": "foaf:Group",
      "attributes": {
        "name": {
          "type": "string",
          "predicate": "foaf:name"
        },
        "identifier": {
          "type": "string",
          "predicate": "dct:identifier"
        }
      },
      "relationships": {
        "users": {
          "predicate": "foaf:member",
          "target": "users",
          "cardinality": "many"
        }
      },
      "features": [
        "include-uri"
      ]
    },
    "accounts": {
      "new-resource-base": "http://themis.vlaanderen.be/id/account/",
      "class": "foaf:OnlineAccount",
      "attributes": {
        "provider": {
          "type": "url",
          "predicate": "foaf:accountServiceHomepage"
        }
      },
      "relationships": {
        "user": {
          "predicate": "foaf:account",
          "target": "users",
          "cardinality": "one",
          "inverse": true
        }
      },
      "features": [
        "include-uri"
      ]
    }
  }
}
